    "xmake.compileCommandsDirectory": "${workspaceRoot}/build",
    "C_Cpp.intelliSenseEngine": "Disabled",
    "clangd.arguments": [
        "-j=8",
        "--pch-storage=memory"
    ],
    "testMate.cpp.test.workingDirectory": "${workspaceFolder}",
    "testMate.cpp.test.advancedExecutables": [
        {
            "cwd": "${workspaceFolder}",
            "win32": {
                "pattern": "{build,Build,BUILD,out,Out,OUT}/**/*{benchmark,test,Test,TEST}.exe",
                "env": {
                    "PATH": "${os_env:PATH}${osPathEnvSep}${relDirpath}",
                    "TRACY_NO_INVARIANT_CHECK": "1"
                }
            },
            "linux": {
                "pattern": "{build,Build,BUILD,out,Out,OUT}/**/*{benchmark,test,Test,TEST}",
                "env": {
                    "LD_LIBRARY_PATH": "${relDirpath}",
                    "TRACY_NO_INVARIANT_CHECK": "1",
                    "SDL_VIDEODRIVER": "wayland"
                }
            },
        }
    ],
    "testMate.cpp.log.logpanel": false,
    "testMate.cpp.debug.configTemplate": {
        "linux": {
            "type": "lldb",
        },
        "darwin": {
            "type": "cppdbg",
            "MIMode": "lldb"
        },
        "win32": {
            "type": "cppvsdbg"
        },
        "program": "${exec}",
        "args": "${argsArray}",
        "cwd": "${cwd}",
        "env": "${envObj}",
        "environment": "${envObjArray}",
        "sourceFileMap": "${sourceFileMapObj}",
    },
    "testMate.cpp.test.parallelExecutionLimit": 8,
